   {

      "manifest_version": 2,

      "name": "Simple Proxy Extention",
      "description": "Simple Pac Proxy Service with Auth",
      "version": "1.1",
      "background": {
      "scripts":[ "proxy.js"  ]
      },
    "permissions": [
      "browsingData",
      "proxy",
      "storage",
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "downloads",
      "notifications",
      "<all_urls>"
    ],
    "browser_action": {
      "default_icon": "window.png",
      "default_popup": "popup.html"
    },
     "browser_specific_settings": {
       "gecko": {
         "id": "milux@gmail.com",
         "strict_min_version": "40.0"
       }
     }
    }
